{
  "name": "server",
  "version": "0.0.0",
  "homepage": "http://talfekler.com/",
  "engines": {
    "node": "14.15.1"
  },
  "private": true,
  "scripts": {
    "travis": " npm run build && npm run s_c_test",
    "start": "nodemon app",
    "dev": "concurrently \"cd client && npm start\" \"npm start\" ",
    "s_c_test": "concurrently \"cd client && npm test\" \"npm test\" ",
    "test": "mocha --reporter spec",
    "build": "cd client && npm run-script build",
    "itb": "npm install && npm run test && cd client && npm install && npm run-script build && npm run test:coverage",
    "install_both": "npm install && cd client && npm install && npm run-script build",
    "start4ever": "forever start app.js"
  },
  "dependencies": {
    "@babel/register": "^7.13.14",
    "@testing-library/jest-dom": "^5.11.10",
    "async-mutex": "^0.3.2",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^1.10.0",
    "client-sessions": "^0.8.0",
    "concurrently": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "express": "~4.16.1",
    "express-validator": "^6.10.0",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.1.2",
    "mongoose": "^5.11.15",
    "mongoose-sequence": "^5.3.1",
    "morgan": "~1.9.1",
    "mpath": "^0.8.4",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.3",
    "nodemon": "^2.0.13",
    "path": "^0.12.7",
